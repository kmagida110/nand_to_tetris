/**
 * Exclusive-or gate:
 * out = not (a == b)
 */

CHIP Xor {
    IN a, b;
    OUT out;

    PARTS:
    Not(in=b,out=NotB);
    Not(in=a,out=NotA);
    And(a=a,b=NotB,out=ANotB);
    And(a=NotA,b=b,out=NotAB);
    Or(a=ANotB,b=NotAB,out=out);
}